{
  "name": "Cart",
  "version": "1.0",
  "description": "A cross-site shopping cart for your universal purchases.",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "browser_action": {
      "default_title": "Web Cart",
      "default_icon": "bucketlist.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
